{
  "name": "dynamic/state-dropdown-field",
  "description": "A simple state dropdown field for SilverStripe forms",
  "license": "BSD-3-Clause",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "dropdown",
    "state",
    "form",
    "field"
  ],
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com",
      "homepage": "https://www.dynamicagency.com"
    }
  ],
  "require": {
    "silverstripe/recipe-cms": "^1.0 || ^4.0 || ^5.0",
    "silverstripe/vendor-plugin": "^1.0 || ^2.0"
  },
  "require-dev": {
    "silverstripe/recipe-testing": "^2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Dynamic\\StateDropdownField\\": "src/",
      "Dynamic\\StateDropdownField\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
